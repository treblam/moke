doctype html
html
    head
        meta(charset='utf-8')
        meta(http-equiv='X-UA-Compatible', content='IE=edge')
        meta(name='viewport', content='width=device-width, initial-scale=1')
        title 跳转中

    body
        script.
            function gup( name ) {
              name = name.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");
              var regexS = "[\\?&]"+name+"=([^&#]*)";
              var regex = new RegExp( regexS );
              var results = regex.exec( window.location.href );
              if( results == null )
                return null;
              else
                return results[1];
            }

            var isSuc = gup('suc') == 1;

            if (window.opener) {
                window.opener.postMessage({ isSuc: isSuc }, 'http://www.moke.io');
                window.opener.focus();
            }
            window.close();